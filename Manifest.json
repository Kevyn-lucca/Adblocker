{
    "manifest_version": 2,
    "name": "X AdBlocker",
    "description": "Blocking ads.",
    "version": "0.0.1",
    "author": "<kevyn>",
    "browser_action": {
        "default_title": "X AdBlocker"
    },
    "permissions": [
        "tabs",
        "webNavigation",
        "https://twitter.com/"
    ],
    "background": {
        "scripts": [
            "extension.js"
        ]
    }
}